{
  "name": "bench",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build-node": "tsc -p src/tsconfig.node.json",
    "pregenerate-files": "npm run build-node",
    "generate-files": "node out-tsc/node/generate.js",
    "pregenerate-files:prod": "cross-env NODE_ENV=production && npm run build-node && cross-env NODE_ENV=development",
